package com.example.secoundproject

fun main()
{
    /*
     var name:String? = readLine()
            OR                                  //for console input
      var name:String = readLine()!!
     */

    print("What is you name? " )
    var name:String? = readLine()

    print("How old are you? ")
    var age:Int = readLine()!!.toInt()

    println("Your name is $name and your age is $age ")

}
