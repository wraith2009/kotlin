package com.example.secoundproject

fun main() {
    /*
    1. =
    2. +=
    3. -=
    4. *=
    5. /=
    6. %=
    */

    var x: Int = 20
    var y: Int = 15
    var z: Int = 0

    z = x + y
    println("1.z=x+y: " + z)

    z += x //z=z=x
    println("2." + z)

    z -= x //z=z-x
    println("3." + z)

    z *= x //z=z*x
    println("4." + z)

    z /= x //z=z/x
    println("5." + z)

    z %= x //z=z%x
    println("6." + z)
}
